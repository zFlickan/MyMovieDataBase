
@{
    ViewBag.Title = "My Movies";
    
}
@model List<MyMovieDataBase.Domain.Movie>
<h2>MyMovies</h2>

<h1>Movie</h1>

<table>
    <tr>
        <th>Title</th>
        <th>Year</th>
    </tr>
    @foreach (var movie in Model)
    {
        <tr>
            <td>@movie.Title</td>
            <td>@movie.Year</td>
        </tr>
    }
</table>